
@using System.Security.Policy
@model System.Collections.Generic.ICollection<Shop.Common.ViewModels.TrackDetailsViewModel>

@{ ViewBag.Title = "Все треки";}
<div class="tracks-list">
    <ul class="list-group" id="slist">
        @foreach (var item in Model)
        {
            @Html.Partial("TrackListItem", item)
            
        }
    </ul>
</div>

<script>
    function itemClick(id) {
        var url = '@Url.Action("Details", "Tracks", new { id = "*" })';
        window.location.href = url.replace('*', encodeURIComponent(id));
    }

    function audioClick() {
        alert("click detected");
    }
</script>
