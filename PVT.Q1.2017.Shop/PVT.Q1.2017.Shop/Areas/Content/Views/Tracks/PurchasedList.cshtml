@model ICollection<Shop.Common.ViewModels.PurchasedTrackViewModel>

@{ ViewBag.Title = "Мои покупки"; }

<div class="purchased-tracklist">
    <div class="tracks-list col-md-12">
        <ul class="list-group">
            @foreach (var item in Model)
            {
                @Html.Partial("_PurchasedTrackListItem", item)
            }
        </ul>
    </div>
</div>


<script src="~/Scripts/get-audio.js"></script>
@section cssStyles{
    <link href="~/Content/tracks.css" rel="stylesheet" />
    <link href="~/Content/ratingStars.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet"/>
}